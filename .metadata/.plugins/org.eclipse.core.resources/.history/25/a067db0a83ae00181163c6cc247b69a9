package project;

public class Method {

	public static void main(String[] args) {
		Method t = new Method();
		
		myMethod();
		t.myName();

	}
	
	public static void myMethod() {
		System.out.println("Hello world");
		
	}
	
	void myName(){
		System.out.println("Hello Java");
	}

}

